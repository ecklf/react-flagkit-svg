{
  "name": "react-flagkit-svg",
  "version": "1.0.2",
  "description": "FlagKit for React (uses svg instead of img tags + typings support)",
  "main": "index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "fetch": "rm -rf SVG/ && svn export https://github.com/madebybowtie/FlagKit/trunk/Assets/SVG",
    "compile": "yarn fetch && npx svgo SVG/*.svg && node build.js && yarn format",
    "format": "prettier --trailing-comma es5 --write 'src/**/*.js'",
    "build:bundle": "rm -rf build && rollup --config rollup.config.js",
    "build:es": "rm -rf dist && babel src --out-dir dist --copy-files",
    "build": "concurrently \"npm:build:*\""
  },
  "repository": "https://github.com/impulse/react-flagkit-svg",
  "keywords": [
    "react",
    "flag",
    "flagkit",
    "svg",
    "design"
  ],
  "author": "impulse",
  "license": "MIT",
  "homepage": "https://github.com/impulse/react-flagkit-svg/#readme",
  "bugs": {
    "url": "https://github.com/impulse/react-flagkit-svg/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.2",
    "prettier": "^1.18.2",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  }
}
